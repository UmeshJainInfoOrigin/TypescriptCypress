{
  "name": "typescriptcypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^16.0.3",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "cypress": "^12.9.0",
   
    "cypress-sql-server": "^1.0.0",
    "mocha": "^10.2.0",
    "multiple-cucumber-html-reporter": "^3.3.0",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "test": "npx cypress run",
    "headTest": "npm run test --headed",
    "chromeTest": "npm run test --browser chrome",
    "cloud": "npm run test --record --key bb9862d7-e9d1-469c-b32f-6ee9fcdfcdf5 --reporter mochawesome --spec cypress/integration/examples/BDD/*.feature",
    "greenKartTest": "npm run test --spec \"cypress/integration/GreenKart/*\"",
    "tag": "npx cypress run --env tags=@healthcheck",
    "env": "npx cypress run --spec cypress/integration/**/*.feature"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "cypress/integration/**/*.spec.ts"
    ],
    "json": {
      "enabled": true,
      "output": "cypress/cucumberReports/results.json"
    }
  },
  "author": "",
  "license": "ISC"
}
